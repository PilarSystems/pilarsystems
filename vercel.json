{
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "installCommand": "yarn install --ignore-scripts",
  "framework": "nextjs",
  "regions": ["fra1"],
  "crons": [
    {
      "path": "/api/autopilot/scheduler",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/autopilot/run",
      "schedule": "0 10 * * *"
    }
  ]
}
